{
  "name": "magic-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "pnpm@8.12.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ibrahim-azez/magic-service"
  },
  "engines": {
    "npm": "Please use pnpm",
    "yarn": "Please use pnpm",
    "node": ">=16.14.0 <=20.18.0",
    "pnpm": ">=8.0.0 <=8.20.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nest start -w --path ./tsconfig.dev.json -b swc --type-check",
    "build:dev": "cross-env NODE_ENV=development nest build --path ./tsconfig.dev.json ",
    "build:stage": "cross-env NODE_ENV=stage nest build --path ./tsconfig.stage.json ",
    "build:prod": "cross-env NODE_ENV=production nest build --path ./tsconfig.prod.json ",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/prod/main",
    "prisma:generate": "prisma generate",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "tsc:check": "tsc --project tsconfig.lint.json --pretty --noEmit",
    "prettier:check": "prettier  --config .prettierrc --ignore-path .prettierignore --check .",
    "prettier:format": "prettier  --config .prettierrc --ignore-path .prettierignore --write .",
    "lint:check": "eslint -c ./.eslintrc.js --ignore-path .eslintignore \"./{src,test,spec}/**/*.ts\" ",
    "lint:fix": "eslint -c ./.eslintrc.js --ignore-path .eslintignore \"./{src,test,spec}/**/*.ts\" --fix",
    "circular:deps:check": "dpdm --no-warning  --no-tree --exit-code circular:1 ./src/main.ts",
    "gitlab:check": " gitlab-ci-local --preview",
    "cleanup": "rm -r node_modules && rm -r dist",
    "reinstall": "rm -r node_modules && pnpm i",
    "commit:push": "git add . && git commit -m \"yolo\" --no-verify && git push",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@fastify/cookie": "9.3.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/static": "7.0.4",
    "@nestjs-cls/transactional": "2.3.1",
    "@nestjs-cls/transactional-adapter-prisma": "1.2.1",
    "@nestjs/common": "10.0.0",
    "@nestjs/core": "10.0.0",
    "@nestjs/platform-express": "10.0.0",
    "@nestjs/platform-fastify": "10.3.9",
    "@nestjs/swagger": "7.3.1",
    "@prisma/client": "5.16.1",
    "@types/qs": "6.9.15",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "fastify-multer": "2.0.3",
    "nestjs-cls": "4.3.0",
    "qs": "6.12.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@nestjs/cli": "10.0.0",
    "@nestjs/schematics": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/supertest": "6.0.0",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "cspell": "8.3.0",
    "dpdm": "3.14.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sort-class-members": "1.20.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "gitlab-ci-local": "4.46.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "15.2.7",
    "prettier": "3.0.0",
    "prisma": "5.16.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3"
  }
}
